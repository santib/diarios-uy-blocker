{
  "manifest_version": 4,

  "name": "Diarios Uruguay Blocker",
  "description": "Previene que los diarios de Uruguay no nos dejen informarnos gratuitamente",
  "version": "4.0",

  "background": { "scripts": ["request-blocker.js"] },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.elpais.com.uy/*",
    "*://*.ovaciondigital.com.uy/*",
    "*://*.elobservador.com.uy/*"
  ]
}
